{
  "expo": {
    "name": "MovieSocial",
    "slug": "moviesocial",
    "scheme": "moviesocial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MS_icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/MS_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MS_icon.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "backgroundColor": "#18181b"
      }
    },
    "web": {
      "favicon": "./assets/images/MS_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": ""
      }
    }
  }
}